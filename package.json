{
  "name": "react-webpack-setup",
  "version": "0.1.0",
  "description": "Standard setup for react and redux production",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:cordova": "webpack --config webpack.cordova.js && cordova build",
    "build:all": "webpack --config webpack.dev.js && webpack --config webpack.prod.js && webpack --config webpack.cordova.js",
    "serve": "webpack-dev-server --https --config webpack.serve.js",
    "serve:cordova": "webpack --config webpack.cordova.js && cordova run"
  },
  "author": "Caio_Aletroca",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.6",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-ga": "^2.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.0-rc.2",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-cordova-plugin": "^0.1.6",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "cordova-android": "^7.0.0"
  },
  "cordova": {
    "platforms": [
      "android"
    ]
  }
}